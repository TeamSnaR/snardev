      },
      "configurations": {
        "production": {
          "tsConfig": "libs/split-app-standalone/shared/utils-bill-pipes/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/split-app-standalone/shared/utils-bill-pipes/tsconfig.lib.json"
