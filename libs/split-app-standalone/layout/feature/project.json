{
  "projectType": "library",
  "sourceRoot": "libs/split-app-standalone/layout/feature/src",
  "prefix": "snardev",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/split-app-standalone/layout/feature"],
      "options": {
        "project": "libs/split-app-standalone/layout/feature/ng-package.json",
        "tailwindConfig": "libs/split-app-standalone/layout/feature/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/split-app-standalone/layout/feature/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/split-app-standalone/layout/feature/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/split-app-standalone/layout/feature"],
      "options": {
        "jestConfig": "libs/split-app-standalone/layout/feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/split-app-standalone/layout/feature/**/*.ts",
          "libs/split-app-standalone/layout/feature/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:split-app-standalone", "type:feature", "context:layout"]
}
