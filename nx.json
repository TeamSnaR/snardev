{
  "npmScope": "snardev",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "M2I2YWIzZGYtNTliMS00NjM5LWJjYWMtZDcyYjc2NmQ5NzUwfHJlYWQ="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true,
      "tags": "type:app",
      "addTailwind": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "routing": true,
      "viewEncapsulation": "None"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "simpleModuleName": true
    },
    "@nrwl/angular:component": {
      "style": "scss",
      "displayBlock": true,
      "changeDetection": "OnPush",
      "viewEncapsulation": "None",
      "flat": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "skipTests": true
    }
  },
  "defaultProject": "foodie-app"
}
